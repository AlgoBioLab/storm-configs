<?xml version="1.0" encoding="ISO-8859-1"?>
<settings>
  <!-- Valve parameters -->
  <valve_com_ports>
	  <!-- Valves must be listed here in commands/protocol order, i.e. the valve referred to by
		  valve_ID "1" in the commands_file should be listed first, and so on. -->
	  <valve1_com_port type="string">COM7</valve1_com_port>
	  <valve2_com_port type="string">COM8</valve2_com_port>
	  <valve3_com_port type="string">COM9</valve3_com_port>
  </valve_com_ports>
  <!--<valves_com_port type="string">COM2</valves_com_port>-->	<!-- COM port of serial connection to valves -->
  <valve_class type="string">valves.elveflow_mux</valve_class>
  <num_simulated_valves type="int">3</num_simulated_valves><!-- Number of valves to simulate (Defaults to 0) -->

  <!-- Pump parameters -->
  <pump_type type="string">pressure</pump_type>
  <pump_class type="string">pumps.elveflow_ob1</pump_class><!-- Control class for pump -->
  <pump_com_port type="string">COM3</pump_com_port><!-- COM port of serial connection to pump -->
  <pump_ID type="int">30</pump_ID><!-- ID of Pump -->
  <simulate_pump type="boolean">False</simulate_pump><!-- Simulate pump? (Defaults to False) -->
  <flip_flow_direction type="boolean">False</flip_flow_direction><!-- Flip the direction defined as forward? -->
  <speed_units type="string">microL/min</speed_units><!-- Pump speed units -->
  <max_speed type="float">100000</max_speed><!-- Max pump speed, in speed_units -->
  <min_speed type="float">50</min_speed><!-- Min pump speed, in speed_units -->

  <!-- General Kilroy parameters -->
  <verbose type="boolean">True</verbose>
  <serial_verbose type="boolean">True</serial_verbose>  <!-- display serial commands? -->
  <tcp_port type="int">9500</tcp_port> <!-- TCP/IP port for local communication with Dave -->
  <protocols_file type = "">default_config.xml</protocols_file><!-- Location of default protocol -->
  <commands_file type = "">default_config.xml</commands_file><!-- Location of default commands -->

</settings>
